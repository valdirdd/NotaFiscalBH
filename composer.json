{
    "name": "valdir/notafiscalbh",
    "description": "Integração com o WebService da Prefeitura de Belo Horizonte para emissão de NFS e NTFS utilizando a API da nota do milhão",
    "type": "library",
    "license": "MIT",
    "require": {
        "robrichards/xmlseclibs": "^3.0",
        "greenter/xmldsig": "^5.0",
        "spatie/array-to-xml": "^2.8",
        "ext-dom": "*",
        "ext-soap": "*"
    },
    "suggest": {
        "ext-mbstring": "*"
    },
    "keywords": [
        "NFS",
        "NFTS",
        "Emissão Nota Fiscal",
        "Nota Fiscal",
        "Nota do Milhão",
        "Prefeitura São Paulo",
        "Nota Fiscal Tomador de Serviços"
    ],
    "authors": [
        {
            "name": "Kaio Valdemar",
            "email": "kaio@e-htl.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "NotaFiscalSP\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
